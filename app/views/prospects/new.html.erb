<h1>Discover what is in for you</h1>
<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <%= form_for(@prospect) do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.label :profile_link %>
      <%= f.text_field :profile_link %>

      <%= f.label :dev_link %>
      <%= f.text_field :dev_link %>

      <%= f.label :name  %>
      <%= f.text_field :name %>

      <%= f.submit "Submit", class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>

