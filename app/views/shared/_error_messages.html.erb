<% if @prospect.errors.any? %>
  <div id="error_explanation">
  <div class="alert alert-danger">
    The form contains <%= pluralize(@prospect.errors.count, "error") %>.
  </div>
  <ul>
    <% @prospect.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
  <% end %>
  </ul>
</div>
<% end %>
